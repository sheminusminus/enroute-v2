{
  "name": "fe-boilerplate",
  "version": "1.0.0",
  "description": "Frontend boilerplate setup",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build:dev": "webpack --progress --colors --config webpack.config",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.prod",
    "dev": "NODE_ENV=development webpack-dev-server --progress --host 0.0.0.0",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|png|svg)$": "<rootDir>/test/__utils__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "SvgInject": "<rootDir>/test/__utils__/componentMock.js"
    },
    "setupFiles": [
      "jest-localstorage-mock",
      "./test/__utils__/requestAnimationFrame.js",
      "./test/__utils__/enzymeConfig.js",
      "./test/__utils__/mockDate.js"
    ]
  },
  "author": "Emily Kolar",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "autoprefixer": "^9.7.3",
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "babel-preset-jest": "^24.9.0",
    "@babel/preset-react": "^7.8.0",
    "css-loader": "^3.4.2",
    "ejs": "^3.0.1",
    "ejs-html-loader": "^4.0.1",
    "ejs-loader": "^0.3.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "json-loader": "^0.5.7",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
